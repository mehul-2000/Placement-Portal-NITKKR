{

    "installCommand": "cd frontend && yarn && yarn build && ls &&  && cd ../backend && npm install",
    "builds":[
        {
            "src":"./backend/server.js",
            "use":"@vercel/node"
        },
        {
            "src":"./frontend/public/*",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"backend/server.js"
        }
    ]
}